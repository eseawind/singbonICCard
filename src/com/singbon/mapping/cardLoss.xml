<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.singbon.dao.CardLossDAO">
	<resultMap id="cardLoss" type="com.singbon.entity.CardLoss">
	</resultMap>
	<parameterMap type="com.singbon.entity.CardLoss" id="cardLoss2" />
	<insert id="insert" parameterMap="cardLoss2" useGeneratedKeys="true" keyProperty="id">
		insert into cardLoss (companyId,cardNO) values (#{companyId},#{cardNO})
	</insert>
	<select id="selectListByCompanyId" resultMap="cardLoss">
		select * from cardLoss where companyId=#{companyId}
	</select>
</mapper>

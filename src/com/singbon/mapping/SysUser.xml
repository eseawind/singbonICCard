<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.singbon.dao.SysUserDAO">
	<resultMap id="sysUser" type="com.singbon.entity.SysUser">
	</resultMap>
	<select id="selectByLoginInfo" resultMap="sysUser">
		select * from sysuser where loginName =#{loginName} and loginPwd =#{loginPwd}
	</select>
</mapper>